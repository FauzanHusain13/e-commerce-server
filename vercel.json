{
  "version": 2,
  "builds": [
    {
      "src": ".",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "./app/user/router.js",
      "methods": ["GET", "POST", "DELETE"],
      "dest": "./app/user/controller.js"
    },
    {
      "src": "./app/auth/router.js",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "dest": "./app/auth/controller.js"
    }
  ]
}
