{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
     { 
      "src": "/app/admin/router", 
      "dest": "/" 
     },
     {
      "src": "/app/dashboard/router",
      "dest": "/dashboard"
     },
     {
      "src": "/app/about/router",
      "dest": "/about"
     },
     {
      "src": "/app/logo/router",
      "dest": "/logo"
     },
     {
      "src": "/app/jumbutron/router",
      "dest": "/jumbotron"
     },
     {
      ""
     }
  ]
}
