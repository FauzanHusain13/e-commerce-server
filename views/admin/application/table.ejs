<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th>No.</th>
      <th>Nama</th>
      <th>Link sosmed</th>
      <th>Aksi</th>
    </tr>
    </thead>
    <tbody>
      <% for(let i = 0; i < application.length; i++) { %>
        <tr>
          <td><%= i + 1 %></td>
          <td><%= application[i].name %></td>
          <td><%= application[i].link %></td>
          <td>
            <form action="/application/delete/<%= application[i]._id %>?_method=DELETE" method="POST">
              <button class="btn btn-danger btn-sm">
                <i class="fas fa-trash"></i>
                Hapus
              </button>
            </form>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>