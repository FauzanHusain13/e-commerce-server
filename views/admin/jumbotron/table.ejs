<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th>No.</th>
      <th>Title</th>
      <th>Sub Title</th>
      <th>Aksi</th>
    </tr>
    </thead>
    <tbody>
      <% for(let i = 0; i < jumbotron.length; i++) { %>
        <tr>
          <td><%= i + 1 %></td>
          <td><%= jumbotron[i].title %></td>
          <td><%= jumbotron[i].subTitle %></td>
          <td>
            <form action="/jumbotron/delete/<%= jumbotron[i]._id %>?_method=DELETE" method="POST">
              <a href="/jumbotron/edit/<%= jumbotron[i]._id %>" class="btn btn-info btn-sm">
                <i class="fas fa-pencil-alt"></i>
                Ubah
              </a>
            </form>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>